{
  "name": "poptarts",
  "version": "1.0.0",
  "description": "Game for Global Game Jam 2019",
  "main": "N/A",
  "scripts": {
    "build": "npm run lint && npm run compile && npm run copystatic && npm run webpack-dev",
    "compile": "tsc",
    "copystatic": "mkdir -p build/controller/css build/screen/css && cp -r src/*.html build && cp -r src/controller/css build/controller && cp -r src/screen/css build/screen",
    "fixlint": "tslint --fix 'src/**/*.ts'",
    "lint": "tslint 'src/**/*.ts'",
    "webpack-dev": "npx webpack --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jven/poptarts.git"
  },
  "author": "",
